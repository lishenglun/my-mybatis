<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.msb.other.tx.dao.UserDao">

  <insert id="addUser">
    insert into
    user(username,password,enable)
    values(
    #{username},
    #{password},
    #{enable}
    );
  </insert>

</mapper>
